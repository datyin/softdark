  "tokenColors": [
    {
      "name": "comment",
      "scope": ["comment", "punctuation.definition.comment"],
      "settings": {
        "foreground": "#436164"
      }
    },
    {
      "name": "constatns",
      "scope": ["constant", "support.constant", "keyword.command", "fenced_code.block"],
      "settings": {
        "foreground": "#ac7c6d"
      }
    },
    {
      "scope": ["keyword.other.unit", "string.other.link.title"],
      "settings": {
        "foreground": "#b4a068"
      }
    },
    {
      "name": "self",
      "scope": ["variable.language.self", "variable.language.this"],
      "settings": {
        "fontStyle": "italic bold"
      }
    },
    {
      "scope": ["meta.function.parameter", "variable.parameter"],
      "settings": {
        "fontStyle": "italic"
      }
    },
    {
      "scope": "markup.inline.raw.string",
      "settings": {
        "foreground": "#7f8b92"
      }
    },
    {
      "name": "variables",
      "scope": [
        "support.constant.variable",
        "variable.prop",
        "meta.embedded.line",
        "meta.embedded.block",
        "variable.other.php",
        "entity.name.type.module"
      ],
      "settings": {
        "foreground": "#99a8b1"
      }
    },
    {
      "scope": [
        "variable.other.object.property",
        "variable.other.property",
        "support.type.property-name",
        "entity.name.tag.yaml"
      ],
      "settings": {
        "foreground": "#8096a5"
      }
    },
    {
      "name": "string",
      "scope": [
        "string",
        "meta.at-rule.import",
        "support.constant.font-name",
        "variable.parameter.url",
        "markup.underline.link"
      ],
      "settings": {
        "foreground": "#6e9769"
      }
    },
    {
      "name": "brackets",
      "scope": [
        "punctuation",
        "entity.name.operator",
        "keyword.operator.type",
        "keyword.operator.combinator",
        "meta.brace",
        "meta.preprocessor.amxxpawn",
        "meta.preprocessor.macro.amxxpawn",
        "markup.heading.setext"
      ],
      "settings": {
        "foreground": "#4e626b"
      }
    },
    {
      "name": "keywords",
      "scope": [
        "keyword.local",
        "keyword.control",
        "keyword.operator",
        "keyword.import",
        "keyword.function",
        "keyword.package",
        "entity.name.tag",
        "storage.type.function",
        "storage.modifier.async",
        "support.type.object.module"
      ],
      "settings": {
        "foreground": "#8c6c9b"
      }
    },
    {
      "name": "reserved",
      "scope": [
        "entity.other.attribute-name",
        "storage.type",
        "storage.modifier",
        "keyword.operator.new",
        "keyword.other.sql",
        "keyword.other.use"
      ],
      "settings": {
        "foreground": "#6292be"
      }
    },
    {
      "name": "types",
      "scope": ["entity.name.type", "support.type.primitive", "keyword.other.type", "support.type.builtin"],
      "settings": {
        "foreground": "#3ba1a1"
      }
    },
    {
      "name": "function",
      "scope": ["entity.name.function", "support.function", "entity.name.package"],
      "settings": {
        "foreground": "#af9d6f"
      }
    },
    {
      "name": "class",
      "scope": [
        "support.class",
        "support.other.namespace",
        "entity.name.class",
        "entity.name.type.class",
        "entity.name.scope-resolution",
        "entity.other.inherited-class",
        "entity.other.attribute-name.pseudo-class",
        "entity.other.attribute-name.pseudo-element"
      ],
      "settings": {
        "foreground": "#a8a464"
      }
    },
    {
      "name": "CSS Class",
      "scope": ["entity.other.attribute-name.class"],
      "settings": {
        "foreground": "#b3627a"
      }
    },
    {
      "name": "CSS ID",
      "scope": ["entity.other.attribute-name.id"],
      "settings": {
        "foreground": "#be71a7"
      }
    }
  ],
  "semanticHighlighting": true,
  "semanticTokenColors": {
    "class": "#a8a464",
    "type": "#48a38f",
    "interface": "#48a38f"
  }
}
